#==========================
# generic develop shortcuts
#==========================
# git conflict
alias gitcH="sed -e '/<<<</,/====/ d' -e '/>>>>/ d'"
alias gitcN="sed -e '/====/,/>>>>/ d' -e '/<<<</ d'"

# json formatter/pretty with python
alias pyjson="python3 -m json.tool"

# date parses
alias gts="date -Iseconds -ur "

# cscope generate
